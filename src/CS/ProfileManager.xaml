<Window x:Class="TPKtaneHelper.src.CS.ProfileManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPKtaneHelper.src.CS"
        mc:Ignorable="d"
        Title="ProfileManager" Width="Auto" Height="Auto" SizeToContent="WidthAndHeight">
    <StackPanel>
        <StackPanel x:Name="profilePanel" Margin="0,0,0,20"/>
        <Button x:Name="createBTN" Click="createBTN_Click"  Content="Create profile" />
        <Button x:Name="doneBTN" Click="doneBTN_Click" Content="Done" />
        <Button x:Name="cancelBTN" Click="cancelBTN_Click" Content="Cancel" />
    </StackPanel>
</Window>
