<Window x:Class="TPKtaneHelper.src.CS.GUI.ModuleChooser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPKtaneHelper.src.CS.GUI"
        mc:Ignorable="d"
        Title="ModuleChooser" Width="Auto" Height="Auto" SizeToContent="WidthAndHeight">
    <StackPanel x:Name="ModulesPanel">
        <TextBox x:Name="moduleID" Text="1" Grid.ColumnSpan="2" />
        <TextBox x:Name="moduleName" Text="" TextChanged="moduleName_TextChanged" Grid.ColumnSpan="2" />
    </StackPanel>
</Window>
