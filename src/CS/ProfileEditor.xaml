<Window x:Class="TPKtaneHelper.src.CS.ProfileEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPKtaneHelper.src.CS"
        mc:Ignorable="d"
        Title="ProfileEditor" Width="400" Height="200" SizeToContent="WidthAndHeight">
    <Grid>
        <TextBlock x:Name="nameBlock" Margin="86,0,10,172" />
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Username: " />

        </StackPanel>
        <TextBox x:Name="tokenBox" Margin="86,18,10,148" />
        <StackPanel Orientation="Horizontal" Margin="0,18,0,-18">
            <TextBlock Text="OAuth token: " />
        </StackPanel>
        <Button Content="Save" Click="SaveButton_Click" Margin="0,110,0,50"/>
        <Button Content="Cancel" Click="CancelButton_Click" Margin="0,139,0,25"/>
    </Grid>
</Window>
