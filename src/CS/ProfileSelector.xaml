<Window x:Class="TPKtaneHelper.src.CS.ProfileSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPKtaneHelper.src.CS"
        mc:Ignorable="d"
        Title="ProfileSelector" Height="150" Width="200">
    <Grid Height="59">
        <Label Content="Please select a profile!" Margin="21,0,-21,111" />
        <ComboBox x:Name="profileBox" Margin="0,5,0,31">
            <ComboBoxItem IsSelected="True" Content="" />
        </ComboBox>
        <Button x:Name="doneBTN" Content="Done" Margin="0,33,0,1" Visibility="Hidden"/>
        <Button Content="Manage profiles" Click="Button_Click" Margin="0,62,0,-34" />
    </Grid>
</Window>
